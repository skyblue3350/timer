  },
  "homepage": "https://github.com/skyblue3350/electron-template-ts-react#readme",
  "dependencies": {
    "css-loader": "^3.4.2",
    "electron-store": "^5.1.1",
    "electron-window-state": "^5.0.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "semantic-ui-css": "^2.4.1",
