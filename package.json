    "electron": "^8.2.0",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/semantic-ui": "^2.2.7",
    "@types/webpack": "^4.41.6",
    "electron-builder": "^22.4.1"
  }
